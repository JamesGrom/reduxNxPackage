import {ACTION_TYPES,Action} from "../index"

export type stateTypeInStore = <%=reducedStateType%>
const initialVal: stateTypeInStore = <%=initialValue%>;
export const <%=actionName%>Reducer = (
	oldVal: typeof initialVal = initialVal,
	action: Action
): stateTypeInStore => {
	switch (action.type) {
		case ACTION_TYPES.<%=uppercase(actionName)%>: {
			// IMPLEMENT REDUCER HERE
			return oldVal;
		}
		default:
			return oldVal;
	}
};