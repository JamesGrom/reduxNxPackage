import {<%=errorName%>Payload } from "./actionPayload";
import {ACTION_TYPES} from "../index"
export const set_<%=errorName%> = (
	payload: <%=errorName%>Payload  
): { type: ACTION_TYPES.<%=uppercase(errorName)%>; payload: <%=errorName%>Payload  } => ({
	type: ACTION_TYPES.<%=uppercase(errorName)%>,
	payload,
});