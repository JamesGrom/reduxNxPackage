import {<%=loaderName%>Payload } from "./actionPayload";
import {ACTION_TYPES} from "../index"
export const set_<%=loaderName%> = (
	payload: <%=loaderName%>Payload  
): { type: ACTION_TYPES.<%=uppercase(loaderName)%>; payload: <%=loaderName%>Payload  } => ({
	type: ACTION_TYPES.<%=uppercase(loaderName)%>,
	payload,
});